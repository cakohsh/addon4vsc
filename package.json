{
  "name": "addon4vsc",
  "displayName": "AddOn4VSC",
  "publisher": "cako",
  "description": "Eine Erweiterung für die C-Vorlesung.",
  "license": "license",
  "repository": {
    "type": "git",
    "url": ""
  },
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "extensionDependencies": [
    "formulahendry.code-runner",
    "ms-vscode.cpptools",
    "ms-vsliveshare.vsliveshare",
    "ms-vsliveshare.vsliveshare-audio"
  ],
  "browser": [
    {
      "name": "Mac",
      "extensionDependencies": [
        "vadimcn.vscode-lldb"
      ]
    },
    {
      "name": "Linux",
      "extensionDependencies": [
        "vadimcn.vscode-lldb"
      ]
    }
  ],
  "activationEvents": [
    "onCommand:exam.start",
    "onStartupFinished"
  ],
  "main": "./out/main.js",
  "contributes": {
    "commands": [
      {
        "command": "exam.start",
        "title": "Starte die Übung!"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4"
  }
}